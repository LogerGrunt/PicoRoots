function _update60()
	if winlose==false then
		if in_tutorial==false then
			if warning==false then
				update_stats()
				move_selection()
				switch_root_type()
				check_build()
				check_root_spr()
			end
		end
		if warning then
		check_warning_btnp()
		end
		if in_tutorial then
			flip_page()
		end
	end
end


function _draw()
	if winlose==false then
		if in_tutorial==false then
			if warning==false then
			cls()
			cam_mov()
			map()
			draw_sun()
			--draw_level()
			draw_selected()
			print_stats()
			show_root_sprite()
			
			print("selected:" .. tile_type(selectionx,selectiony),7)
			print("root type:",camx+80,camy+2,7)
			
			end
			show_warning()

			
		end	
	end
	check_winlose()
	?mget(selectionx,selectiony)
end


function check_warning_btnp()
	if (btnp(5)) close_warning()
end


